<?xml version = "1.0" encoding = "UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
	<bean id="employee" class="com.basic.containers.beans.Employee">
		<constructor-arg name="name"
			value="Hello! My name is Java" />
		<constructor-arg name="age" value="18" />
	</bean>
	<bean id="address" class="com.basic.containers.beans.Address">
		<constructor-arg name="city"
			value="Hyderabad" />
		<constructor-arg name="zipcode" value="500001" />
	</bean>
</beans>